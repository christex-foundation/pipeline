		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/vite": "^4.0.13",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"svelte": "^4.2.7",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.13",
		"typescript": "^5.0.0",
